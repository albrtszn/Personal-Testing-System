                            </Grid.ColumnDefinitions>

                            <Image Source="{StaticResource messageDrawingImage}" Width="20" Height="20"/>
                            <TextBlock Grid.Column="1" Text="Сообщения от пользователей" VerticalAlignment="Center"  Margin="4.5 0 0 0" FontSize="16"/>
                        </Grid>
                    </ListBoxItem>
                    <ListBoxItem x:Name="LB8" Selected="itemLogs_Selected" Width="280" Height="46" >
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Image Source="{StaticResource system_logDrawingImage}" Width="20" Height="20"/>
                            <TextBlock Grid.Column="1" Text="Журнал операций" VerticalAlignment="Center"  Margin="4.5 0 0 0" FontSize="16"/>
                        </Grid>
                    </ListBoxItem>
                </ListBox>



                <ListBox x:Name="ListSetting" Style="{DynamicResource ListBoxStyle2}" Grid.Row="1"  Width="300" HorizontalAlignment="Center" FontSize="16" FontFamily="Roboto" Foreground="#EDEDED" >
                    <ListViewItem MouseUp="Setting"  Width="280" Height="46">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Image Source="{StaticResource fluent_settings_16_regularDrawingImage}" Width="22" Height="22"/>
                            <TextBlock Grid.Column="1" Text="Настройка" VerticalAlignment="Center" Margin="4.5 0 0 0"/>
                        </Grid>

                    </ListViewItem>

                    <ListViewItem  Width="280" Height="46" MouseUp="About">
                        <Grid >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Image Source="{StaticResource mdi_information_outlineDrawingImage}" Width="22" Height="22"/>
                            <TextBlock Grid.Column="1" Text="О приложении" VerticalAlignment="Center" Margin="4.5 0 0 0"/>
                        </Grid>

                    </ListViewItem>

                    <ListViewItem Selected="ListViewItem_Selected_Exit" Width="280" Height="46">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Image Source="{StaticResource majesticons_logout_lineDrawingImage}" Width="16" Height="16"/>
                            <TextBlock Grid.Column="1" Text="Завершить сеанс" VerticalAlignment="Center" Margin="4.5 0 0 0"/>
                        </Grid>

                    </ListViewItem>

                </ListBox>


            </Grid>



            <Grid Background="#EDEDED" Grid.Column="2" Grid.Row="1">

                <Frame x:Name="adminFrame" Source="../pages/PageUsers.xaml" NavigationUIVisibility="Hidden"/>

            </Grid>

            <Border x:Name="BRMess" Background="White" Grid.Column="2" Grid.Row="1" Width="600" Height="300" Visibility="Collapsed">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition/>
                        <RowDefinition Height="80"/>
                    </Grid.RowDefinitions>

                    <Button Click="CloseQuestion" Grid.Row="0" Width="16" Height="16" Style="{StaticResource MaterialDesignFlatButton}" Padding="0" Background="Transparent" Opacity="1" Margin="0 15 15 15 "
                        HorizontalAlignment="Right"
                        >
                        <Image Source="{StaticResource Frame_69DrawingImage}" Width="16" Height="16"/>
                    </Button>

                    <TextBlock x:Name="TBMess" Grid.Row="1"
                           
                           TextWrapping="Wrap"
                           FontFamily="Roboto" FontSize="18" Margin="20 20 20 20"
                           />

                    <Button Grid.Row="2" Width="150" Height="40" Content="ПРОЧИТАНО" FontFamily="Roboto" FontSize="16" Background="#F25C4F" BorderBrush="Transparent"
                        materialDesign:ButtonAssist.CornerRadius="20"
                        Click="Button_Click_Read"
                        />
                </Grid>
            </Border>

        </Grid>
    </Border>
</Window>
