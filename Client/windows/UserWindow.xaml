    <Border Background="White" MouseDown="Border_MouseDown">


        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="320"/>
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="50" />
                <RowDefinition />
            </Grid.RowDefinitions>

            <Border Background="#141623" Grid.RowSpan="2"/>
            <Image Source="/Images/backAdmin.png" Grid.RowSpan="2" Width="320" Height="630" VerticalAlignment="Top"/>
            <Image Source="{StaticResource Group_18386DrawingImage}" Width="25.22" Height="26.42" Margin="37 23 0 0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Label Content="Competencies Exam" FontSize="9.918" Foreground="#EDEDED" FontFamily="/Fonts/#Terminator Cyr" Margin="68.63 29 0 0"/>
            <Rectangle Fill="#323551" Grid.Row="1" Grid.Column="0" Margin="20,48,0,0" Width="280" MinHeight="90" Height="{Binding ElementName=SPUser, Path=ActualHeight, Converter={StaticResource myHome}, ConverterParameter=20}" RadiusX="20" RadiusY="20" Opacity="0.5"  HorizontalAlignment="Left" VerticalAlignment="Top">
                
            </Rectangle>
            <Image Source="{StaticResource Group_496DrawingImage}" Grid.RowSpan="2" Width="54" Height="54" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="35 115 0 0"/>
            <StackPanel x:Name="SPUser" Grid.Row="1" VerticalAlignment="Top" Margin="100 65 25 0" >
                <TextBlock x:Name="textBlock_UserName1" Grid.Row="1" Text="Иванов" VerticalAlignment="Top" Foreground="#EDEDED" FontSize="16"/>
                <TextBlock x:Name="textBlock_UserName2" Grid.Row="1" Text="Иван Иванович" VerticalAlignment="Top" Foreground="#EDEDED" FontSize="16"/>
                <TextBlock x:Name="textBlock_UserName3" Grid.Row="1" Text="Слесарь" VerticalAlignment="Top" Foreground="#808080" FontSize="12" TextWrapping="Wrap"/>
            </StackPanel>

            <Grid Background="White" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="200"/>
                </Grid.ColumnDefinitions>



                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0 0 25 0" Grid.Column="1">

                    <Button Click="WindowMinimizeApp" Width="16" Height="16" Style="{StaticResource MaterialDesignFlatButton}" Padding="0" Background="Transparent" Opacity="1" Margin="0 0 50 0">
                        <Image Source="{StaticResource Frame_70DrawingImage}" Width="16" Height="16"/>
                    </Button>

                    <Button Click="WindowMaximizeApp" Width="16" Height="16" Style="{StaticResource MaterialDesignFlatButton}" Padding="0" Background="Transparent" Opacity="1" Margin="0 0 50 0">
                        <Image Source="{StaticResource Frame_72DrawingImage}" Width="16" Height="16"/>
                    </Button>

                    <Button Click="CloseApp" Width="16" Height="16" Style="{StaticResource MaterialDesignFlatButton}" Padding="0" Background="Transparent" Opacity="1" Margin="0 0 5 0">
                        <Image Source="{StaticResource Frame_69DrawingImage}" Width="16" Height="16"/>
                    </Button>

                </StackPanel>

            </Grid>

            <Grid Background="Transparent" Grid.RowSpan="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="230"/>
                </Grid.RowDefinitions>

                <ListBox Style="{DynamicResource ListBoxStyle1}" Width="300" HorizontalAlignment="Center" FontSize="15" FontFamily="Roboto" Foreground="#EDEDED" Margin="0 241 0 0"
                         SelectedIndex="0"
                         >

                    <ListBoxItem x:Name="LB1" Selected="item0_Selected" Width="280" Height="46" >
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Image Source="{StaticResource listDrawingImage}" Width="20" Height="20"/>
                            <TextBlock Grid.Column="1" Text="Перечень тестов" VerticalAlignment="Center" FontSize="16"  Margin="4.5 0 0 0"/>
                        </Grid>
                    </ListBoxItem>
                    <ListBoxItem x:Name="LB2" Selected="item1_Selected" Width="280" Height="46" >
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Image Source="{StaticResource personDrawingImage}" Width="20" Height="20"/>
                            <TextBlock Grid.Column="1" Text="Персональные данные" VerticalAlignment="Center"  Margin="4.5 0 0 0"/>
                        </Grid>
                    </ListBoxItem>


                    <ListViewItem  x:Name="LB3" Width="280" Height="46" Selected="selecAddMass">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Image Source="{StaticResource supportDrawingImage}" Width="22" Height="22"/>
                            <TextBlock Grid.Column="1" Text="Поддержка" VerticalAlignment="Center" Margin="4.5 0 0 0"/>
                        </Grid>

                    </ListViewItem>
                </ListBox>



                <ListBox x:Name="ListSetting" Style="{DynamicResource ListBoxStyle2}" Grid.Row="1"  Width="300" HorizontalAlignment="Center" FontSize="16" FontFamily="Roboto" Foreground="#EDEDED" 
                         
                         >

                    
                    <ListViewItem  Width="280" Height="46">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Image Source="{StaticResource fluent_settings_16_regularDrawingImage}" Width="22" Height="22"/>
                            <TextBlock Grid.Column="1" Text="Настройка" VerticalAlignment="Center" Margin="4.5 0 0 0"/>
                        </Grid>

                    </ListViewItem>

                    <ListViewItem MouseUp="About" Width="280" Height="46">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Image Source="{StaticResource mdi_information_outlineDrawingImage}" Width="22" Height="22"/>
                            <TextBlock Grid.Column="1" Text="О приложении" VerticalAlignment="Center" Margin="4.5 0 0 0"/>
                        </Grid>

                    </ListViewItem>

                    <ListViewItem Selected="ListViewItem_Selected_Exit" Width="280" Height="46">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Image Source="{StaticResource majesticons_logout_lineDrawingImage}" Width="16" Height="16"/>
                            <TextBlock Grid.Column="1" Text="Завершить сеанс" VerticalAlignment="Center" Margin="4.5 0 0 0"/>
                        </Grid>

                    </ListViewItem>

                </ListBox>


            </Grid>



            <Grid Background="#EDEDED" Grid.Column="2" Grid.Row="1">

                <Frame x:Name="FrameUserData" Source="../pages/PageUserHome.xaml" NavigationUIVisibility="Hidden"></Frame>

            </Grid>


        </Grid>
    </Border>
</Window>

